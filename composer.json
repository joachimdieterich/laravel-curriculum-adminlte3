{
    "name": "joachimdieterich/curriculum",
    "type": "project",
    "description": "curriculum (laravel)",
    "version": "1.0.0",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2.0",
        "aacotroneo/laravel-saml2": "*",
        "bacon/bacon-qr-code": "^2.0",
        "barryvdh/laravel-dompdf": "^2.0",
        "barryvdh/laravel-snappy": "^1.0.0",
        "binarytorch/larecipe": "^2.2",
        "bugsnag/bugsnag-laravel": "^2.15",
        "cmgmyr/messenger": "^2.20",
        "darkaonline/l5-swagger": "^8.3",
        "doctrine/dbal": "^3.0",
        "guzzlehttp/guzzle": "^7.4",
        "h4cc/wkhtmltoimage-amd64": "0.12.x",
        "h4cc/wkhtmltopdf-amd64": "0.12.x",
        "hisorange/browser-detect": "^4.4",
        "intervention/image": "^2.5",
        "jamesdordoy/laravelvuedatatable": "^1.3",
        "joisarjignesh/bigbluebutton": "^2.6",
        "laravel/framework": "^11.0",
        "laravel/passport": "^12.0",
        "laravel/telescope": "^5.0",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^4.0",
        "laravolt/avatar": "^5.1",
        "maatwebsite/excel": "^3.1",
        "maize-tech/laravel-markable": "^2.0",
        "mews/purifier": "^3.4",
        "profburial/wkhtmltopdf-binaries-osx": "0.12.1",
        "select2/select2": "^4.0",
        "yajra/laravel-datatables-oracle": "~11.0"
    },
    "require-dev": {
        "beyondcode/laravel-dump-server": "^2.0",
        "fakerphp/faker": "^1.20",
        "filp/whoops": "^2.0",
        "laracasts/cypress": "^3.0",
        "laravel/dusk": "^8.0",
        "laravel/pint": "^0.2.1",
        "laravel/sail": "^1.16",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "~8.1.0",
        "phpunit/phpunit": "^10.0",
        "phpunit/phpunit-selenium": ">=1.2",
        "roquie/laravel-dusk-select2": "^2.0",
        "roave/security-advisories": "dev-latest"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "files": [
            "app/Helpers/Helper.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "classmap": [
          "database/seeders",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "files": [
            "app/Helpers/Helper.php"
        ],
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "authors" : [{
                    "name" : "Joachim Dieterich",
                    "email" : "mail@joachimdieterich.com",
                    "homepage": "http://www.curriculumonline.de",
                    "role": "Developer"
		}
	]
}
